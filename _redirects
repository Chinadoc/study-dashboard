# Static assets - serve directly (no SPA fallback)
/js/*  /js/:splat  200
/css/*  /css/:splat  200
/images/*  /images/:splat  200
/assets/*  /assets/:splat  200
/public/*  /public/:splat  200

# Auth endpoints: transparent proxy (200) - lets Worker's 302 redirect pass through to browser
/api/auth/*  https://euro-keys.jeremy-samuels17.workers.dev/api/auth/:splat  200

# SPA fallback - all other routes serve index.html
/*  /index.html  200


/api/assets/*  /assets/:splat  200
