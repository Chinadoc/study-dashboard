        {
            "type": "step_group",
            "id": "audi-add-key-megamos48",
            "title": "Procedure: Add Key (Megamos 48, Pre-2006)",
            "estimated_time": "15-20 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Connect VVDI2 or Autel to OBD."
                },
                {
                    "number": 2,
                    "text": "Navigate: IMMO → VAG → Audi → Model."
                },
                {
                    "number": 3,
                    "text": "Read Immobilizer Data (CS/PIN)."
                },
                {
                    "number": 4,
                    "text": "Generate new key using M-Box or VVDI Key Tool."
                },
                {
                    "number": 5,
                    "text": "Write transponder to blank HU66 key."
                },
                {
                    "number": 6,
                    "text": "Learn key via OBD. Test start."
                }
            ]
        },
