        {
            "type": "step_group",
            "id": "hyundai-autel-pin-read",
            "title": "Procedure: Read PIN Code via OBD (Autel)",
            "estimated_time": "2 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Connect to OBD. Navigate: IMMO → Hyundai → Model/Year."
                },
                {
                    "number": 2,
                    "text": "Select 'Read Immobilizer Password'. Turn Ignition ON."
                },
                {
                    "number": 3,
                    "text": "Tool communicates with ECU/BCM."
                },
                {
                    "number": 4,
                    "text": "6-digit PIN is displayed. WRITE IT DOWN.",
                    "tip": "If read fails, use NASTF VSP service ($24)."
                }
            ]
        },
        {
            "type": "step_group",
            "id": "hyundai-add-smart-key",
            "title": "Procedure: Add Smart Key (Autel)",
            "estimated_time": "5 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Select 'Smart Key Learning'. Turn Ignition OFF."
                },
                {
                    "number": 2,
                    "text": "Open and close driver door."
                },
                {
                    "number": 3,
                    "text": "Enter the 6-digit PIN code."
                },
                {
                    "number": 4,
                    "text": "Within 5 seconds, hold the back of new Smart Key against START button.",
                    "tip": "Listen for beep or watch instrument cluster lights flash."
                },
                {
                    "number": 5,
                    "text": "Repeat for additional keys."
                },
                {
                    "number": 6,
                    "text": "Exit programming and test functions."
                }
            ]
        },
        {
            "type": "step_group",
            "id": "hyundai-akl-smart-key",
            "title": "Procedure: All Smart Keys Lost",
            "estimated_time": "15-20 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Select 'All Smart Keys Lost'. Tool will attempt to read PIN or ask for manual entry."
                },
                {
                    "number": 2,
                    "text": "Warning: 'This will erase all existing keys'. Confirm."
                },
                {
                    "number": 3,
                    "text": "Follow same learning sequence: Ignition OFF, Door Open/Close, Hold Key to Start Button."
                },
                {
                    "number": 4,
                    "text": "Program at least 2 keys for best results."
                }
            ]
        },
