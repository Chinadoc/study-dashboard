        {
            "type": "step_group",
            "id": "hyundai-lishi-hy22",
            "title": "Procedure: Picking/Decoding HY22 Lock",
            "estimated_time": "5-10 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Insert HY22 Lishi (Quad Lifter). Apply light tension clockwise.",
                    "tip": "Quad lifter means 2 tracks on top, 2 on bottom."
                },
                {
                    "number": 2,
                    "text": "Pick binding wafers. Once open, decode cuts 1-6 for BOTH left and right tracks."
                },
                {
                    "number": 3,
                    "text": "Cut key on LXP90 / KK10 blank."
                }
            ]
        }
    ]
