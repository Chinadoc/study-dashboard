        {
            "type": "step_group",
            "id": "honda-add-key-type4",
            "title": "Procedure: Add Key (Type 4/5, 2011-2022)",
            "estimated_time": "15-20 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Connect to OBD. Navigate: IMMO → Honda → Model → Year."
                },
                {
                    "number": 2,
                    "text": "Read PIN code from BCM (most tools support this)."
                },
                {
                    "number": 3,
                    "text": "Select 'Add Key'. Enter PIN when prompted."
                },
                {
                    "number": 4,
                    "text": "Insert NEW key into ignition when prompted."
                },
                {
                    "number": 5,
                    "text": "Wait for confirmation. Test start."
                }
            ]
        },
