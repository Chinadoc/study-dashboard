        {
            "type": "step_group",
            "id": "ford-obp-add-key",
            "title": "Procedure: On-Board Add Key (PATS II - Keyed)",
            "estimated_time": "2 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Insert Key 1 (Working), turn to RUN (not START)"
                },
                {
                    "number": 2,
                    "text": "Wait for theft light to go out (3-5 sec), turn OFF and remove"
                },
                {
                    "number": 3,
                    "text": "Within 10 seconds: Insert Key 2 (Working), turn to RUN"
                },
                {
                    "number": 4,
                    "text": "Wait for theft light to go out, turn OFF and remove"
                },
                {
                    "number": 5,
                    "text": "Within 10 seconds: Insert NEW Key, turn to RUN"
                },
                {
                    "number": 6,
                    "text": "Theft light stays OFF or glows steady for 3 sec to confirm"
                }
            ]
        },
        {
            "type": "step_group",
            "id": "ford-obp-rke-only",
            "title": "Procedure: Remote Keyless Entry (RKE) Only",
            "estimated_time": "2 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Close all doors, open driver door. Press UNLOCK on door panel."
                },
                {
                    "number": 2,
                    "text": "Turn ignition from OFF to RUN 8 times in 10 seconds (end in RUN)."
                },
                {
                    "number": 3,
                    "text": "Door locks cycle to confirm mode."
                },
                {
                    "number": 4,
                    "text": "Press any button on first remote. Cycle confirm."
                },
                {
                    "number": 5,
                    "text": "Repeat for additional remotes (up to 8 total)."
                }
            ]
        },
        {
            "type": "step_group",
            "id": "ford-rh850-procedure",
            "title": "Procedure: 2021+ F-150/Bronco (RH850 BCM)",
            "estimated_time": "45-60 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Remove BCM from passenger footwell. Connect to XP400 Pro via APB131."
                },
                {
                    "number": 2,
                    "text": "Extract and save D-FLASH (Processor R7F701573)."
                },
                {
                    "number": 3,
                    "text": "Reinstall BCM. Connect Autel to OBD."
                },
                {
                    "number": 4,
                    "text": "Select 'Key Learning' and LOAD saved D-FLASH to bypass security."
                }
            ]
        },
        {
            "type": "warning_banner",
            "id": "ford-fnv2-active-alarm",
            "level": "critical",
            "content": "2021+ FNV2 ACTIVE ALARM: The Gateway Module (GWM) actively monitors CAN bus for unauthorized wake-up signals. Attempting key programming triggers ALARM + LOCKOUT. You MUST use Active Alarm Bypass Cable or FDRS with NASTF credentials."
        },
