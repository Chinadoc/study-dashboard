        {
            "type": "warning_banner",
            "id": "ford-frequency-trap",
            "level": "critical",
            "content": "Frequency Trap: Keys may look identical but differ in frequency. 2017-2022 models mix 315 MHz and 902 MHz. 2024+ Explorer moved to 434 MHz. ALWAYS verify FCC ID."
        },
        {
            "type": "tool_checklist",
            "id": "ford-essential-tools",
            "title": "Ford Essential Tools",
            "items": [
                {
                    "name": "Autel IM608 / IM508",
                    "required": true,
                    "notes": "Primary tool for OBD programming"
                },
                {
                    "name": "XP400 Pro + APB131",
                    "required": false,
                    "notes": "Required for 2021+ RH850 BCM reading (F-150, Bronco)"
                },
                {
