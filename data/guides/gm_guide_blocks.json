        {
            "type": "info_callout",
            "id": "gm-can-fd-barrier",
            "level": "important",
            "content": "CAN FD Physical Layer Barrier: Standard CAN = 500 kbps, 8 bytes/frame. CAN FD = up to 5 Mbps, 64 bytes/frame. Legacy tools physically cannot interpret these frequencies. They hear 'static noise' when connected to Global B vehicles."
        },
        {
            "type": "warning_banner",
            "id": "gm-24-digit-token",
            "level": "critical",
            "content": "24-DIGIT SECURITY TOKEN MECHANISM: Tool requests security access → Gateway generates encrypted session-specific seed → Seed MUST be transmitted to central server (GM's or aftermarket like Autel) → Server validates VIN, calculates 24-digit response → Token sent back to BCM. Without server connection = LOCKED. No offline bypass possible."
        },
        {
            "type": "step_group",
            "id": "gm-global-b-akl",
            "title": "Procedure: Global B All Keys Lost (Autel IM608)",
            "estimated_time": "45-60 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Verify vehicle is Global B (check VIN 12th digit, visual cues, RPO J22 code)"
                },
                {
                    "number": 2,
                    "text": "Connect CAN FD Adapter between VCI and OBD port",
                    "warning": "Without adapter = communication error"
                },
                {
                    "number": 3,
                    "text": "Ensure stable internet connection for server calculation"
                },
                {
                    "number": 4,
                    "text": "Select 'CANFD Smart Key' menu in Autel software"
                },
                {
                    "number": 5,
                    "text": "Tool reads BCM data and sends request to Autel cloud"
                },
                {
                    "number": 6,
                    "text": "Wait 5-10 minutes for 24-digit token calculation"
                },
                {
                    "number": 7,
                    "text": "Follow prompts to program keys"
                },
                {
                    "number": 8,
                    "text": "If alarm active, silence it first - Gateway may block OBD port during alarm"
                }
            ]
        },
        {
            "type": "step_group",
            "id": "gm-official-sps2",
            "title": "Procedure: Official SPS2 Method (Guaranteed)",
            "estimated_time": "30-45 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Connect GM MDI2 (or J2534 passthrough like DrewTech Cardaq Plus 3) to vehicle"
                },
                {
                    "number": 2,
                    "text": "Launch Techline Connect with SPS2 application on Windows PC"
                },
                {
                    "number": 3,
                    "text": "Log in with GM Dealer credentials or NASTF VSP ID"
                },
                {
                    "number": 4,
                    "text": "Software reads VIN, verifies VSP credentials"
                },
                {
                    "number": 5,
                    "text": "24-digit token exchange handled automatically in background"
                },
                {
                    "number": 6,
                    "text": "Follow on-screen prompts for 'Add Key' or 'Program IMMO'"
                }
            ]
        },
