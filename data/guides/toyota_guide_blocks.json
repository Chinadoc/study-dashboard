        {
            "type": "step_group",
            "id": "toyota-add-key-4d67",
            "title": "Procedure: Add Key (4D67 System, 2005-2010)",
            "estimated_time": "10-15 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Connect to OBD. Navigate: IMMO → Toyota → Model → Year."
                },
                {
                    "number": 2,
                    "text": "Select 'Add Transponder Key'."
                },
                {
                    "number": 3,
                    "text": "Turn ignition ON with working key when prompted."
                },
                {
                    "number": 4,
                    "text": "Insert NEW key when prompted. Wait for confirmation."
                },
                {
                    "number": 5,
                    "text": "Turn OFF, test start with new key."
                }
            ]
        },
        {
            "type": "step_group",
            "id": "toyota-prox-add-key",
            "title": "Procedure: Add Proximity Key (2014+ H-Chip)",
            "estimated_time": "15-20 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "Verify fob: Correct FCC ID and chip type (H / 128-bit)."
                },
                {
                    "number": 2,
                    "text": "Connect to OBD. Select IMMO → Toyota → Smart Key."
                },
                {
                    "number": 3,
                    "text": "Select 'Smart Key Learning'. Tool communicates with BCM."
                },
                {
                    "number": 4,
                    "text": "Hold NEW fob against steering column antenna when prompted."
                },
                {
                    "number": 5,
                    "text": "Wait for 'Key Registered' message."
                },
                {
                    "number": 6,
                    "text": "Test proximity unlock, lock, and push-button start."
                }
            ]
        },
