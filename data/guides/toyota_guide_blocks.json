        {
            "type": "step_group",
            "id": "toyota-16min-reset-procedure",
            "title": "Procedure: 16-Minute Smart Code Reset (Legacy 2004-2017)",
            "estimated_time": "20-25 mins",
            "steps": [
                {
                    "number": 1,
                    "text": "VOLTAGE: Connect battery maintainer (12.5V+ mandatory). LS 460 especially sensitive to voltage drops."
                },
                {
                    "number": 2,
                    "text": "CONNECT: Plug into OBD-II. Navigate: IMMO → Toyota → Smart Key → Smart Code Reset."
                },
                {
                    "number": 3,
                    "text": "INITIATE: Select 'Reset'. Tool displays 16-minute countdown. Security indicator will flash."
                },
                {
                    "number": 4,
                    "text": "WAIT: Do NOT touch brake pedal, start button, or door locks during countdown."
                },
                {
                    "number": 5,
                    "text": "CONFIRMATION: Tool beeps at completion. All previous keys are now ERASED."
                },
                {
                    "number": 6,
                    "text": "REGISTER: Hold NEW key emblem against Start Button. Listen for 'Bip' (detected) then 'Bip-Bip' (registered)."
                },
                {
                    "number": 7,
                    "text": "EXIT LEARNING: Place last key on passenger seat. Cycle driver's door (Open/Close) to exit learning mode."
                }
            ]
        },
