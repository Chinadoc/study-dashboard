{
  "version": 2,
  "name": "study-dashboard",
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "gunnaiza.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/amazon-product", "dest": "/api/amazon-product.js" },
    { "src": "/api/amazon-search", "dest": "/api/amazon-search.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
