{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/api/amazon-product",
      "dest": "/api/amazon-product.js"
    },
    {
      "src": "/api/amazon-search",
      "dest": "/api/amazon-search.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    }
  }
}